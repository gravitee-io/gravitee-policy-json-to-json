{
  "id": "pre-jolt-spec-with-el",
  "name": "pre-jolt-spec-with-el",
  "gravitee": "2.0.0",
  "properties" : [ {
    "key" : "my-prop",
    "value" : "my-value",
    "encrypted" : false
  } ],
  "proxy": {
    "context_path": "/pre-jolt-spec-with-el",
    "endpoints": [
      {
        "name": "default",
        "target": "http://localhost:8080/team",
        "http": {
          "connectTimeout": 3000,
          "readTimeout": 60000
        }
      }
    ]
  },
  "flows": [
    {
      "name": "flow-1",
      "methods": [
      ],
      "enabled": true,
      "path-operator": {
        "path": "/",
        "operator": "STARTS_WITH"
      },
      "pre": [
        {
          "name": "Json to Json",
          "description": "",
          "enabled": true,
          "policy": "json-to-json",
          "configuration": {
            "scope": "REQUEST",
            "specification": "[{ \"operation\": \"default\", \"spec\": { \"my-prop\": \"{#properties['my-prop']}\"}}]"
          }
        }
      ],
      "post": [
      ]
    }
  ],
  "resources": []
}
